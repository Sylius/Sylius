<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="sylius.behat.page.admin.locale.create.class">Sylius\Behat\Page\Admin\Locale\CreatePage</parameter>
        <parameter key="sylius.behat.page.admin.locale.index.class">%sylius.behat.page.admin.crud.index.class%</parameter>
    </parameters>

    <services>
        <service id="sylius.behat.page.admin.locale.create" class="%sylius.behat.page.admin.locale.create.class%" parent="sylius.behat.page.admin.crud.create">
            <argument type="string">sylius_admin_locale_create</argument>
        </service>
        <service id="Sylius\Behat\Page\Admin\Locale\CreatePageInterface" alias="sylius.behat.page.admin.locale.create" />

        <service id="sylius.behat.page.admin.locale.index" class="%sylius.behat.page.admin.locale.index.class%" parent="sylius.behat.page.admin.crud.index">
            <argument type="string">sylius_admin_locale_index</argument>
        </service>
        <service id="Sylius\Behat\Page\Admin\Locale\IndexPageInterface" alias="sylius.behat.page.admin.locale.index" />
    </services>
</container>
