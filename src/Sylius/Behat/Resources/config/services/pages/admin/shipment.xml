<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="sylius.behat.page.admin.shipment.index.class">Sylius\Behat\Page\Admin\Shipment\IndexPage</parameter>
        <parameter key="sylius.behat.page.admin.shipment.show.class">Sylius\Behat\Page\Admin\Shipment\ShowPage</parameter>
