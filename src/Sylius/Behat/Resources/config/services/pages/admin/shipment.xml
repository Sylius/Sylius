        <service id="sylius.behat.page.admin.shipment.show" class="%sylius.behat.page.admin.shipment.show.class%" parent="sylius.behat.symfony_page" />
        <service id="Sylius\Behat\Page\Admin\Shipment\ShowPageInterface" alias="sylius.behat.page.admin.shipment.show" />
    </services>
