<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service
            id="sylius.behat.element.admin.product_association_type.form"
            class="Sylius\Behat\Element\Admin\ProductAssociationType\FormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>

        <service
            id="sylius.behat.element.admin.product_attribute.form"
            class="Sylius\Behat\Element\Admin\ProductAttribute\FormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>

        <service
            id="sylius.behat.element.admin.product.association_form"
            class="Sylius\Behat\Element\Admin\Product\AssociationsFormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>

        <service
            id="sylius.behat.element.admin.product.attributes_form"
            class="Sylius\Behat\Element\Admin\Product\AttributesFormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>

        <service
            id="sylius.behat.element.admin.product.channel_pricing_form"
            class="Sylius\Behat\Element\Admin\Product\ChannelPricingsFormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>

        <service
            id="sylius.behat.element.admin.product.media_form"
            class="Sylius\Behat\Element\Admin\Product\MediaFormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>

        <service
            id="sylius.behat.element.admin.product.taxonomy_form"
            class="Sylius\Behat\Element\Admin\Product\TaxonomyFormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>

        <service
            id="sylius.behat.element.admin.product.translations_form"
            class="Sylius\Behat\Element\Admin\Product\TranslationsFormElement"
            parent="sylius.behat.element.admin.crud.form"
        >
            <argument type="service" id="Sylius\Behat\Service\Helper\AutocompleteHelperInterface" />
        </service>
    </services>
</container>
