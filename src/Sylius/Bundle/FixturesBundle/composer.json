{
    "name": "sylius/fixtures-bundle",
    "type": "symfony-bundle",
    "description": "Configurable fixtures for Symfony2 applications.",
    "keywords": ["sylius", "fixtures", "symfony"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Kokot",
            "homepage": "http://kamil.kokot.me"
        },
        {
            "name": "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name": "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "doctrine/data-fixtures": "^1.1",
        "monolog/monolog": "^1.8",
        "symfony/framework-bundle": "^2.8",
        "symfony/monolog-bridge": "^2.8",
        "webmozart/assert": "^1.0",
        "zendframework/zend-stdlib": "^3.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "pamil/phpspec-generator-extension": "^0.4",
        "phpspec/phpspec": "^3.0",
        "phpunit/phpunit": "^5.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Bundle\\FixturesBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
