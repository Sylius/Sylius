<fieldset>
    {{ form_row(form.name, {'attr': {'class': 'input-lg'}}) }}
    {{ form_row(form.parent, {'attr': {'class': 'input-xlarge'} }) }}
    {{ form_row(form.permalink, {'attr': {'class': 'input-xlarge'} }) }}
    {{ form_row(form.file) }}
    <br>
    {{ form_row(form.description) }}
</fieldset>

{{ form_rest(form) }}
