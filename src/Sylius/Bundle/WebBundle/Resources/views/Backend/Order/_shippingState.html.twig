{% set state = order.shippingState %}
{% set label = 'primary' %}

{% if 'backorder' == state or 'partially_shipped' == state %}
    {% set label = 'warning' %}
{% elseif 'shipped' == state %}
    {% set label = 'success' %}
{% elseif 'returned' == state or 'dispatched' == state %}
    {% set label = 'info' %}
{% endif %}

<span class="label label-{{ label }}">{{ state }}</span>
