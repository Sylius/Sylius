{
    "name": "sylius/web-bundle",
    "type": "symfony-bundle",
    "description": "Sylius store web bundle.",
    "keywords": ["shop", "ecommerce", "store", "webshop", "sylius"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name":     "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name":     "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name":     "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",

        "jms/translation-bundle": "1.1.*",
        "knplabs/knp-menu-bundle": "1.1.*",
        "knplabs/knp-snappy-bundle": "*@dev",
        "liip/imagine-bundle": "~1.0",
        "sylius/core-bundle": "0.16.*",
        "symfony/framework-bundle": "^2.7.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Bundle\\WebBundle\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.16-dev"
        }
    }
}
