{% block subject %}
    {{ 'sylius.emails.contact_request.subject'|trans }}
{% endblock %}

{% block body %}
    {% autoescape %}
        {{ 'sylius.emails.contact_request.body.message_from'|trans({'%email%': data.email})|raw }}
        <br/>
        {{ 'sylius.emails.contact_request.body.message_label'|trans|raw }}
        <br/>
        "{{ data.message }}"
    {% endautoescape %}
{% endblock %}
