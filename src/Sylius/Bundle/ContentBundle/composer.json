{
    "name": "sylius/content-bundle",
    "type": "symfony-bundle",
    "description": "Sylius content bundle.",
    "keywords": ["shop", "ecommerce", "store", "webshop", "sylius"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name": "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name": "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "jackalope/jackalope-doctrine-dbal": "^1.1.2",
        "sylius/resource-bundle": "^1.0",
        "symfony/framework-bundle": "^2.8",
        "symfony-cmf/content-bundle": "^1.3",
        "symfony-cmf/routing-bundle": "^1.4"
    },
    "require-dev": {
        "liip/imagine-bundle": "^1.0",
        "phpspec/phpspec": "^3.0",
        "symfony/form": "^2.8"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Bundle\\ContentBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
