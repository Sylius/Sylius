{
    "name": "sylius/taxonomy-bundle",
    "type": "symfony-bundle",
    "description": "Flexible categorization system for Symfony2.",
    "keywords": ["shop", "ecommerce", "taxonomies", "taxonomy", "category", "categorization", "sylius"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name":     "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name":     "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name":     "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^5.5.9|^7.0",

        "jms/translation-bundle": "1.1.*",
        "stof/doctrine-extensions-bundle": "~1.1",
        "sylius/taxonomy": "0.18.*",
        "sylius/resource-bundle": "0.18.*",
        "symfony/framework-bundle": "^2.7.7"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "doctrine/orm": "^2.4.8,<2.5",
        "symfony/form": "^2.7",
        "symfony/validator": "^2.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Bundle\\TaxonomyBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Sylius\\Bundle\\TaxonomyBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.18-dev"
        }
    }
}
