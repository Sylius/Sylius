{
    "name": "sylius/grid-bundle",
    "type": "symfony-bundle",
    "description": "Amazing grids with support of filters and custom fields integrated into Symfony.",
    "keywords": ["shop", "ecommerce", "store", "webshop", "sylius", "grid", "admin", "crud"],
    "homepage": "http://sylius.com",
    "license": "MIT",
    "authors": [
        {
            "name":     "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name":     "Sylius project",
            "homepage": "http://sylius.com"
        },
        {
            "name":     "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^7.1",

        "sylius/grid": "^1.0",
        "symfony/form": "^3.2",
        "symfony/framework-bundle": "^3.2",
        "symfony/templating": "^3.2"
    },
    "require-dev": {
        "akeneo/phpspec-skip-example-extension": "^3.0@alpha",
        "doctrine/doctrine-bundle": "~1.3",
        "doctrine/orm": "^2.5",
        "doctrine/phpcr-odm": "^1.3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "jackalope/jackalope-doctrine-dbal": "^1.2",
        "matthiasnoback/symfony-config-test": "^3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^2.0",
        "ocramius/proxy-manager": "^2.1",
        "pagerfanta/pagerfanta": "^1.0",
        "phpspec/phpspec": "^4.0",
        "phpunit/phpunit": "^6.5",
        "polishsymfonycommunity/symfony-mocker-container": "^1.0",
        "symfony/browser-kit": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/proxy-manager-bridge": "^3.3",
        "symfony/security-csrf": "^3.2",
        "symfony/twig-bundle": "^3.2",
        "symfony/validator": "^3.2",
        "twig/twig": "^2.0"
    },
    "conflict": {
        "twig/twig": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Bundle\\GridBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "Sylius\\Bundle\\GridBundle\\spec\\": "spec/",
            "AppBundle\\": "test/src/AppBundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "incenteev-parameters": {
            "file": "test/app/config/parameters.yml"
        }
    }
}
