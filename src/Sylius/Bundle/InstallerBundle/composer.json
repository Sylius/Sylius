{
    "name": "sylius/installer-bundle",
    "type": "symfony-bundle",
    "description": "Web and command line installer for Sylius.",
    "keywords": ["shop", "ecommerce", "installer", "install"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name":     "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name":     "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name":     "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.9",

        "sylius/core-bundle": "0.16.*",
        "symfony/framework-bundle": "^2.7.5"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "symfony/form": "~2.3",
        "symfony/validator": "~2.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Bundle\\InstallerBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Sylius\\Bundle\\InstallerBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.16-dev"
        }
    }
}
