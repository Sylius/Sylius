{% set main_hook = hook_name(hookable_data.parent_main_hook, 'latest_statistics') %}
{% set fallback_hook = hook_name(hookable_data.parent_fallback_hook, 'latest_statistics') %}

<div class="container-xl">
    <div class="row">
        {% hook [main_hook, fallback_hook] with {
            parent_main_hook: main_hook,
            parent_fallback_hook: fallback_hook,
        } %}
    </div>
</div>
