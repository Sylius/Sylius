{% set main_hook = hook_name(hookable_data.parent_main_hook, 'more') %}
{% set fallback_hook = hook_name(hookable_data.parent_fallback_hook, 'more') %}

<div class="dropdown">
    <button class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
        {{ 'sylius.ui.more'|trans }}
    </button>
    <ul class="dropdown-menu">
        {% hook [main_hook, fallback_hook] with hookable_data|merge({
            parent_main_hook: main_hook,
            parent_fallback_hook: fallback_hook,
        }) %}
    </ul>
</div>
