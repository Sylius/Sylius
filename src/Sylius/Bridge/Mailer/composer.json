    "autoload-dev": {
        "psr-4": { "Sylius\\Bridge\\Mailer\\spec\\": "spec/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    },
    "minimum-stability": "dev"
}
