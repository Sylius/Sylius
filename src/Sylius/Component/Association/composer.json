{
    "name": "sylius/archetype",
    "type": "library",
    "description": "Component for creating product associations for up or cross selling",
    "keywords": ["shop", "ecommerce", "association", "feature"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
