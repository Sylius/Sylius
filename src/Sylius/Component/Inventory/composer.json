{
    "name": "sylius/inventory",
    "type": "library",
    "description": "Flexible inventory management for PHP applications.",
    "keywords": [
        "shop",
        "ecommerce",
        "stock",
        "inventory",
        "sylius"
    ],
    "homepage": "https://sylius.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "https://pjedrzejewski.com"
        },
        {
            "name": "Sylius project",
            "homepage": "https://sylius.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "sylius/resource": "^1.9",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.13-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sylius\\Component\\Inventory\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sylius\\Component\\Inventory\\spec\\": "spec/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
