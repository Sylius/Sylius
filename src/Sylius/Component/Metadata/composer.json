{
    "name": "sylius/metadata",
    "type": "library",
    "description": "Metadata management for PHP applications.",
    "keywords": ["metadata"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Kokot",
            "homepage": "http://kamil.kokot.me"
        },
        {
            "name": "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name": "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "sylius/resource": "^0.19",
        "symfony/property-access": "^2.8",
        "symfony/options-resolver": "^2.8",
        "twig/twig": "~1.11"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "phpunit/phpunit": "^4.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Component\\Metadata\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Sylius\\Component\\Metadata\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.19-dev"
        }
    }
}
