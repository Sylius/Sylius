{
    "name": "sylius/channel",
    "type": "library",
    "description": "E-Commerce PHP component for managing different sales channels.",
    "keywords": ["channel", "multichannel", "multistore", "stores", "ecommerce", "sylius"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name":     "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name":     "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name":     "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^5.5.9|^7.0",

        "doctrine/orm": "^2.4.8,<2.5",
        "sylius/resource": "0.16.*"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Component\\Channel\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Sylius\\Component\\Channel\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.16-dev"
        }
    }
}
