###> symfony/framework-bundle ###
APP_ENV=dev
APP_DEBUG=1
APP_SECRET=EDITME
###< symfony/framework-bundle ###

###> doctrine/doctrine-bundle ###
DATABASE_HOST=mysql
DATABASE_PORT=3306
DATABASE_ROOT_PASSWORD=root
DATABASE_NAME=sylius 
DATABASE_USER=sylius
DATABASE_PASSWORD=nopassword
